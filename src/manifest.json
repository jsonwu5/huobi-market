{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/jsonwu5/huobi-market",
  "description": "__MSG_extDes__",
  "default_locale": "zh_CN",
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "__MSG_extDefTitle__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  }
}
